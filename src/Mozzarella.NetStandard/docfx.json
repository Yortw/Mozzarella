{
  "metadata": [
			{
				"src": [
					{
						"files": [ "**.cs" ],
						"src": "..",
						"exclude": [
							"**/obj/**",
							"**/bin/**",
							"../docs/_site/**",
							"**Tests**"
						]
					}
				],
				"dest": "api",
				"properties": {
					"TargetFramework": "netstardard1.1"
				}
			}
  ],
	"build": {
		"content": [
			{
							"files": [
								"api/**.yml",
								"api/index.md"
							]
			},
			{
				"files": [
					"articles/**.md",
					"articles/**/toc.yml",
					"toc.yml",
					"*.md"
				],
				"exclude": [
					"obj/**",
					"../docs/_site/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**"
				],
				"exclude": [
					"obj/**",
					"../../docs/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md"
				],
				"exclude": [
					"obj/**",
					"../../docs/**"
				]
			}
		],
		"dest": "../../docs",
		"globalMetadataFiles": [],
			"globalMetadata": {
				"_appTitle": "Mozzarella",
				"_appFaviconPath": "images/MozzarellaIcon.ico",
				"_appLogoPath": "images/MozzarellaIconSmall.png",
				"_appFooter": "Copyright (c) 2017 Troy Willmot"
			},
		"fileMetadataFiles": [],
			"template": [ "statictoc", "memberpage/2.24.0/content" ],
		"postProcessors": [],
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false
	}
}